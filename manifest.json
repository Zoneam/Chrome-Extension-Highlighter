{
  "manifest_version": 3,
  "name": "Element Highlighter",
  "version": "1.0",
  "description": "Highlights elements with a red color when hovered over.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"],
      "css": ["highlight.css"]
    }
  ]
}
